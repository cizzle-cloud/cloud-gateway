{
    "middleware_groups": {
        "auth_group": ["auth_middleware"],
        "ratelimit_group": ["auth_middleware", "ratelimit_middleware"],
        "all_group": ["auth_middleware", "nocache_middleware"]
    },
    "routes": [
      {
        "prefix": "/auth",
        "middleware": ["auth_middleware"],
        "paths": [
          {
            "method": "GET",
            "path": "/logout",
            "proxy_target": "http://my-service/users"
          },
          {
            "method": "GET",
            "path": "/signin",
            "proxy_target": "http://my-service/users"
          }
        ]
      },
      {
        "prefix": "/api",
        "middleware_group": "ratelimit_group",
        "method": "POST",
        "proxy_target": "http://my-service/api"
      }
    ]
}